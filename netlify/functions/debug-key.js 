// netlify/functions/debug-key.js

exports.handler = async () => {
  const keyStatus = process.env.STRIPE_SECRET_KEY
    ? `Key is loaded. Length: ${process.env.STRIPE_SECRET_KEY.length} chars`
    : "Key is missing!";

  return {
    statusCode: 200,
    body: JSON.stringify({ status: keyStatus }),
  };
};
