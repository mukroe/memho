exports.handler = async function() {
  return {
    statusCode: 200,
    body: JSON.stringify({
      stripeKeyMode: process.env.STRIPE_SECRET_KEY?.startsWith("sk_live_") 
        ? "LIVE KEY DETECTED" 
        : process.env.STRIPE_SECRET_KEY?.startsWith("sk_test_") 
          ? "TEST KEY DETECTED" 
          : "NO VALID KEY FOUND",
      keyPrefix: process.env.STRIPE_SECRET_KEY?.slice(0, 8) || "NONE"
    })
  };
};
